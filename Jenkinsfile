pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Alfred03092003/Jenkins.git'
            }
        }

        stage('Build') {
            steps {
                bat """
                    if not exist out mkdir out
                    javac -d out src\\Main.java
                """
            }
        }

        stage('Run') {
            steps {
                bat 'java -cp out Main'
            }
        }
    }
}
















































































































































































































































